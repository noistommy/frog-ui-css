doctype html
html
    head
        meta(charset="UTF-8")
        meta(name="viewport",
            content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0")
        meta(http-equiv="X-UA-Compatible", content="ie=edge")
        block title
        link(rel="preconnect", href="https://cdn.jsdelivr.net", crossorigin="anonymous")
        link(rel="preload",
            as="style",
            crossorigin,
            href="https://cdn.jsdelivr.net/gh/orioncactus/pretendard@v1.3.9/dist/web/variable/pretendardvariable-dynamic-subset.css")
        link(rel="stylesheet",
            href="https://cdn.jsdelivr.net/gh/orioncactus/pretendard@v1.3.9/dist/web/variable/pretendardvariable-dynamic-subset.css")
        script(src="/prism/prism.js")
        link(href="/prism/prism.css", rel="stylesheet")
        link(href="/frogui.css", rel="stylesheet")
        link(href="/css/style.css", rel="stylesheet")
    body
        .side-menu
            .background
            .menu-wrapper
                include lnb
        header.gnb
            .logo 
                span FROG 
                //- div.sub-logo Fix, Refactor, Optimize, Grow!!
                div.sub-logo Frontend Renders On Grid...
            nav.main-menu
                .toggle-menu
                    .ga-button.icon.circle.show-menu
                        i.xi-bars
            nav.sub-menu
                .mode
                    .ga-button.icon.circle.toggle-mode
                        i.xi-sun-o
                .github
                    a.ga-button.icon.circle.toggle-mode(href="https://github.com/noistommy/frog-ui-css")
                        i.xi-github
        .main-container.container.main
            .right-part
                block contents
        block module
        block js-module
        
        script(src="/js/base.js")