doctype html
html
    head
        meta(charset="UTF-8")
        meta(name="viewport",
            content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0")
        meta(http-equiv="X-UA-Compatible", content="ie=edge")
        link(rel="shortcut icon", href="/favicon.ico", type="image/x-icon")
        link(rel="icon", href="/favicon.ico", type="image/x-icon")
        block title
        link(rel="preconnect", href="https://cdn.jsdelivr.net", crossorigin="anonymous")
        link(rel="preload",
            as="style",
            crossorigin,
            href="https://cdn.jsdelivr.net/gh/orioncactus/pretendard@v1.3.9/dist/web/variable/pretendardvariable-dynamic-subset.css")
        link(rel="stylesheet",
            href="https://cdn.jsdelivr.net/gh/orioncactus/pretendard@v1.3.9/dist/web/variable/pretendardvariable-dynamic-subset.css")
        script(src="/prism/prism.js")
        link(href="/prism/prism.css", rel="stylesheet")
        link(href="/frogui.css", rel="stylesheet")
        link(href="/css/style.css", rel="stylesheet")
    body
        .container.center
            header.gnb
                nav.main-menu
                    .toggle-menu
                        .ga-button.icon.circle.show-menu
                            i.xi-bars
                .logo 
                    a(href="/")
                        span FROG 
                    //- div.sub-logo Fix, Refactor, Optimize, Grow!!
                    //- div.sub-logo Frontend Renders On Grid...
                    div.sub-logo For Real, Only Good!!
                nav.sub-menu.small
                    //- .home
                    //-     .ga-button.text Home
                    //-         a.link(href="/")
                    //- .components
                    //-     .ga-button.text Components
                    //-         a.link(href="/pages/base")
                    .link-btn
                        .ga-button.circle.badge.compact
                            | Be-UI 
                            .in-badge.ga-deepblue React
                        .ga-button.circle.badge.compact
                            | Be-UI 
                            .in-badge.ga-green vue
                            a.link(href="https://beui.vercel.app/", target="_blank") 
                    .divide
                        .ga-button.text |
                    .mode
                        .ga-button.icon.circle.toggle-mode
                            i.xi-sun-o
                    .github
                        a.ga-button.icon.circle.toggle-mode(href="https://github.com/noistommy/frog-ui-css")
                            i.xi-github
            .main-container.main
                .left-part
                    if title !== 'Frog'
                        .side-menu
                            include lnb
                .right-part
                    block contents
        block module
        block js-module
        
        script(src="/js/base.js")
        script.
            window.va = window.va || function () { (window.vaq = window.vaq || []).push(arguments); };
        script(defer, src="/_vercel/insights/script.js")